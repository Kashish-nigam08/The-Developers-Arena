   "source": [
    "from datetime import datetime\n",
    "current_hour=datetime.now().hour\n",
    "\n",
    "if 5<=current_hour<12:\n",
    "    print(\"Good Morning!\")\n",
    "elif 12<=current_hour<17:\n",
    "    print(\"Good Afternoon!\")\n",
    "elif 17<=current_hour<21:\n",
    "    print(\"Good Evening!\")\n",
    "else:\n",
    "    print(\"Good Night!\")"
   ]
